{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${credits}"
  ],
  "contact": {
    "sources": "${display_url}",
    "issues": "${issue_tracker_url}",
    "homepage": "${homepage_url}"
  },
  "license": "${license}",
  "icon": "${logo_file}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "whocraft.tardis_refined.fabric.TardisRefinedFabric"
    ],
    "cardinal-components": [
      "whocraft.tardis_refined.common.capability.fabric.TRComponents",
      "whocraft.tardis_refined.common.capability.player.fabric.TardisPlayerComponents"
    ],
    "jei_mod_plugin": [
      "whocraft.tardis_refined.compat.jei.TRJeiPlugin"
    ],
    "client": [
      "whocraft.tardis_refined.fabric.TardisRefinedFabricClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "minecraft": "${fabric_minecraft_version_range}",
    "fabric-api": "${fabric_api_version_range}"
  },
  "suggests": {
    "imm_ptl_core": "${fabric_immersive_portals_version_range}"
  },
  "custom": {
    "cardinal-components": [
      "${mod_id}:tardis_data",
      "${mod_id}:tardis_player_info"
    ]
  }
}
